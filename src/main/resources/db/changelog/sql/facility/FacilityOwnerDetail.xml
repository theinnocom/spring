<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
     https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">

    <changeSet id="1745066974831" author="vijayan">
        <createTable tableName="facility_owner_detail">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="owner_name" type="VARCHAR(255)"/>
            <column name="address_line" type="VARCHAR(255)"/>
            <column name="alter_address_line" type="VARCHAR(255)"/>
            <column name="city" type="VARCHAR(255)"/>
            <column name="state" type="VARCHAR(255)"/>
            <column name="zipcode" type="VARCHAR(50)"/>
            <column name="email" type="VARCHAR(255)"/>
            <column name="mobile_phone" type="VARCHAR(50)"/>
            <column name="alter_mobile_phone" type="VARCHAR(50)"/>
            <column name="fax_no" type="VARCHAR(50)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>
