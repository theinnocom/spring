<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="1702706944061" author="Vijayan">
        <createTable tableName="type_of_rpz">
            <column name="type_id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="type" type="varchar(300)"/>
        </createTable>

        <loadData tableName="type_of_rpz"
                  file="db/changelog/data/type_of_rpz.csv"
                  separator=";"
                  usePreparedStatements="false">
            <column name="type_id" type="bigint"/>
            <column name="type" type="varchar(300)"/>
        </loadData>
    </changeSet>
</databaseChangeLog>